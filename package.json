{
  "name": "gatsby-lona-docs-theme",
  "version": "0.1.0",
  "description": "A Gatsby theme used for the Lona documentation websites.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "develop": "GATSBY_PATH_PREFIX=__lona-docs__/secret/prefix GATSBY_DOCS_PATH=./example/docs.json gatsby build --prefix-paths"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lona/lona-docs-github-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "lona",
    "gatsby"
  ],
  "author": "Mathieu Dutour",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "fs-extra": "^8.1.0",
    "gatsby": "^2.18.8",
    "gatsby-cli": "^2.8.18",
    "gatsby-core-utils": "^1.0.22",
    "gatsby-plugin-mdx": "^1.0.59",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-styled-components": "^3.1.14",
    "gatsby-source-filesystem": "^2.1.40",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.0.4",
    "@types/react-helmet": "^5.0.14",
    "@types/styled-components": "^4.4.0",
    "typescript": "^3.7.2"
  }
}
